1)// Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int a ;
    cin >> a   ;
    
    
    if (a ==5 ) {
    cout << "A";
    
    }
    else if ( a == 4) {
        cout << " B";
    }
    else if ( a==3) {
        cout << "C"; 
    }
    else if (a== 2) {
        cout << "E";
    }
    else if (a == 1){
        cout << "F";
    }
    else {
        cout << "oshibka";
    }
    
    
}
2) // Example program
#include <iostream> 
using namespace std;
int main() {

    int m;
    cin >> m;
	switch (m)
	{
	case 1:
	case 2:
	case 12:
		cout << "zima";
		break;
	case 3:
	case 4:
	case 5:
		cout << "vesna";
		break;
	case 6:
	case 7:
	case 8:
		cout << "leto";
		break;
	case 9:
	case 10:
	case 11:
		cout << "oseni";
		break;
	}
}
3) // Example program
#include <iostream> 
using namespace std;
int main() {

    int m;
    cin >> m;
	switch (m)
	{
	case 1:
		cout << "+";
		break;
	case 2:

		cout << "-";
		break;
	case 3:

		cout << "*";
		break;
	case 4:

		cout << "/";
		break;
	}
}
4) // Example program
#include <iostream> 
using namespace std;
int main() {

    int m,n;
    cin >> m;
    cin >> n;
	switch (m)
	{
	case 1:
        cout << n;
        break;
    case 2:
        cout << n / 1000000;
        break;
    case 3:
        cout << n / 1000;
            break;
    case 4:
        cout << n * 1000;
        break;
    case 5:
        cout << n * 100;
        break;
	}
	
}
5) // Example program
#include <iostream> 
using namespace std;
int main() {

    int m,n;
    cin >> m;
    cin >> n;
	switch (n)
    {
    case 6:
        cout << "6 ";
        break;
    case 7:
        cout << "7 ";
        break;
    case 8:
        cout << "8 ";
        break;
    case 9:
        cout << "9 ";
        break;
    case 10:
        cout << "10 ";
        break;
    case 11:
        cout << "valet ";
        break;
    case 12:
        cout << "dama ";
        break;
    case 13:
        cout << "king ";
        break;
    case 14: 
        cout << "tus ";
        break;
    }
    switch (m)
    {
    case 1:
        cout << "pick ";
        break;
    case 2:
        cout << "kresti ";
        break;
    case 3:
        cout << "buben ";
        break; 
    case 4:
        cout << "chervi ";
        break;
    }

}
